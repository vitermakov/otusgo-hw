{
  "serviceId": "sender",
  "serviceName": "Otus Calendar Sender",
  "logger": {
    "fileName": "/var/log/sender.log",
    "level": "info"
  },
  "notify": {
    "queueListen": "userEvents"
  },
  "apiLogin": "ivan@otus.ru",
  "api": {
    "calendar": {
      "type": "grpc",
      "address": "127.0.0.1:8088"
    }
  },
  "mpq": {
    "type": "rabbitMq",
    "rabbitMq": {
      "host": "127.0.0.1",
      "user": "otus_user",
      "password": "otus_pass",
      "port": 5672
    }
  },
  "mailer": {
    "type": "stdout",
    "defaultFrom": "support@otus.ru",
    "templatePath": "/app/templates/mail"
  }
}

