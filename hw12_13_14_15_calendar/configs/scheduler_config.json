{
  "serviceId": "scheduler",
  "serviceName": "Otus Calendar Scheduler",
  "logger": {
    "fileName": "./logs/scheduler.log",
    "level": "info"
  },
  "api": {
    "calendar": {
      "type": "grpc",
      "address": "127.0.0.1:8096"
    }
  },
  "mpq": {
    "type": "rabbitMq",
    "rabbitMq": {
      "host": "127.0.0.1",
      "user": "username",
      "password": "password",
      "port": 5672
    }
  },
  "cleanup": {
    "checkingTime": "1d",
    "storeTime": "1y"
  },
  "notify": {
    "checkingTime": "5s",
    "queuePublish": "userEvents"
  }
}